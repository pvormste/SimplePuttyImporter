<Window x:Class="SimplePuttyImporter.ExportWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Export PuTTY Settings" Height="300" Width="300" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Grid>
        <Button Content="Close" HorizontalAlignment="Left" Margin="185,226,0,0" VerticalAlignment="Top" Width="75" Click="BtnClose_Clicked"/>
        <Button Content="Export" HorizontalAlignment="Left" Margin="105,226,0,0" VerticalAlignment="Top" Width="75" Click="btnExport_Clicked"/>
        <Label Content="Available PuTTY settings:" HorizontalAlignment="Left" Margin="20,22,0,0" VerticalAlignment="Top"/>
        <ListBox x:Name="SettingsBox" HorizontalAlignment="Left" Height="145" Margin="20,53,0,0" VerticalAlignment="Top" Width="240"/>

    </Grid>
</Window>
